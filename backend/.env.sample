# Backend environment template for Windows deployments
# Copy to backend/.env and customize per environment.

# Server
NODE_ENV=production
PORT=3000

# MongoDB
MONGODB_URI=mongodb://localhost:27017/pranaflow

# JWT
JWT_SECRET=change-me
JWT_EXPIRATION=30d

# MinIO / File Storage
MINIO_ENDPOINT=localhost
MINIO_PORT=9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=pranaflow-storage

# Optional: file upload constraints (in bytes)
MAX_UPLOAD_SIZE=10485760
